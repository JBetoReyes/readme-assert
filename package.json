{
  "name": "readme-assert",
  "version": "5.0.3",
  "engines": {
    "node": ">=4"
  },
  "description": "Run code blocks in your readme as test",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "bin": "cli.js",
  "scripts": {
    "build": "babel src/. -d lib/. --ignore=spec.js",
    "test:readme": "babel-node src/cli.js -r babel-register -m ./src ",
    "test:spec": "run-tests -r babel-register src/**/*.spec.js",
    "test": "npm-run-all lint test:*",
    "lint": "eslint src",
    "prepublish": "npm run build"
  },
  "author": {
    "name": "Sigurd Fosseng",
    "email": "sigurd@fosseng.net",
    "url": "http://laat.io"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/laat/readme-assert.git"
  },
  "dependencies": {
    "assert-simple-tap": "^1.0.1",
    "babel-core": "^6.4.5",
    "babel-plugin-transform-comment-to-assert": "^3.0.0",
    "babel-plugin-transform-rename-import": "^2.0.0",
    "docopt": "~0.6.2",
    "eslint": "^3.19.0",
    "gfm-code-blocks": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "eslint": "^3.0.1",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "npm-run-all": "^4.0.2",
    "run-tests": "^1.0.0"
  },
  "files": [
    "lib",
    "cli.js"
  ]
}
